@model IEnumerable<FY111.Models.FY111.ClassUnitApp>

@{ ViewData["Title"] = @Localizer["ClassDownload"]; }

<h1>@ViewData["Title"]</h1>
<hr />

<table class="table table-striped">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Application)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Content)
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
<tr>
    <td>
        @Html.DisplayFor(modelItem => item.Name)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.Application, "ToFile", new { belong = "Application" })
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.Content)
    </td>
</tr>
}
    </tbody>

</table>